            <div class="artist-calendar">
                      <div id="bookings">
          <!-- <#% raise %> -->
                <%#= link_to "<", date: @date.prev_month %>
                <h2 id='month'><%= @date.strftime("%B %Y") %></h2>
                <%#= link_to ">", date: @date.next_month %>

                <%= calendar @date do |date| %>
                  <%= date.day %>
                  <% if @time_blocks_by_date %>
                  <% if @time_blocks_by_date[date.to_date] %>
                  <ul>
                    <% @time_blocks_by_date[date.to_date].each do |booking| %>
                      <li><%= link_to booking.consumer_request.consumer.first_name, consumer_consumers_events_path(booking.consumer_request)  %> - <%= booking.date.strftime("%H %M")%></li>

                      <% end %>
                  </ul>
                  <% end %>

                <% end %>
                <% end %>
              </div>
